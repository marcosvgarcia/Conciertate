<%= form_for @concert do |f| %>
  <p>
    <%= f.label :band %><br>
    <%= f.text_field :band %>
  </p>

  <p>
    <%= f.label :venue %><br>
    <%= f.text_area :venue %>
  </p>

  <p>
    <%= f.label :city %><br>
    <%= f.text_area :city %>
  </p>

  <p>
    <%= f.label :date %><br>
    <%= f.text_area :date %>
  </p>

  <p>
    <%= f.label :price %><br>
    <%= f.text_area :price %>
  </p>

  <p class="stylemarino">
   <%= x.label :photo %><br>
   <%= x.file_field :photo %><br>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :tickets %><br>
    <%= f.text_area :tickets %>
  </p>



  <p>
    <%= f.submit %>
  </p>
<% end %>